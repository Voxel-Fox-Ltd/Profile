msgstr "Genehmigen"

#: cogs/_profile_verificationnnn.py:67
msgid "Decline"
msgstr "Ablehnen"

#: cogs/_profile_verificationnnn.py:331
#, python-brace-format
msgid "Failed to get template with ID `{template_id}`."
msgstr "Die Vorlage mit der ID `{template_id}` konnte nicht abgerufen werden."

